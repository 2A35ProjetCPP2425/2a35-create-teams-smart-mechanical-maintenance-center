    background-color: #ffffff; /* White background */
    border: 1px solid #d0d5db;
    padding: 5px;
    border-radius: 5px;
    color: #333;
}

QLineEdit:focus {
    border: 1px solid #0066cc; /* Blue border on focus */
}

/* QDateEdit Style */
QDateEdit {
    background-color: #ffffff; /* White background */
    border: 1px solid #d0d5db;
    padding: 5px;
    border-radius: 5px;
    color: #333;
}

/* QPushButton Style */
QPushButton {
    background-color: #0066cc; /* Blue background */
    color: #ffffff; /* White text */
    border-radius: 8px;
    padding: 8px 15px;
    font-weight: bold;
}

QPushButton:hover {
    background-color: #005bb5; /* Darker blue on hover */
}

QPushButton:pressed {
    background-color: #004b99; /* Even darker blue on press */
}

/* QTableView Style */
QTableView {
    background-color: #ffffff; /* White background */
    border: 1px solid #d0d5db;
    gridline-color: #cccccc;
    font-size: 14px;
}

