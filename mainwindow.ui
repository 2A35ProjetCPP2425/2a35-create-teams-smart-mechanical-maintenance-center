                 <string notr="true">/* Style général pour la fenêtre principale */
QWidget {
    background: #f0f3f5; /* Fond clair */
    font-family: 'Segoe UI', Arial, sans-serif;
    font-size: 14px;
    color: #333; /* Texte sombre */
}

/* Style des onglets */
QTabWidget::pane {
    border: 1px solid #ccc;
    border-radius: 8px;
    background: #ffffff;
}

QTabBar::tab {
    background: #4e8cff;
    color: #ffffff;
    padding: 8px 20px;
    border-radius: 8px;
    margin: 3px;
    font-weight: bold;
}

QTabBar::tab:selected {
    background: #375e97;
    color: #ffffff;
}

QTabBar::tab:hover {
    background: #6aa6ff;
    color: #ffffff;
}

/* Style des champs de saisie */
QLineEdit, QComboBox, QSpinBox {
    background: #ffffff;
    border: 2px solid #ddd;
    border-radius: 8px;
    padding: 5px 10px;
    font-size: 14px;
}

QLineEdit:focus, QComboBox:focus, QSpinBox:focus {
    border: 2px solid #4e8cff;
    background: #eef7ff;
}

/* Style des boutons */
QPushButton {
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:1, stop:0 #4e8cff, stop:1 #375e97);
    color: #ffffff;
    border: 2px solid #3a3f55;
    border-radius: 8px;
    padding: 10px 20px;
    font-size: 14px;
    font-weight: bold;
    transition: all 0.3s ease-in-out;
}

QPushButton:hover {
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:1, stop:0 #6aa6ff, stop:1 #4e8cff);
    transform: scale(1.05);
}

QPushButton:pressed {
    background: #2d4d7a;
    transform: scale(0.95);
}

/* Style des tableaux */
QTableView {
    background: #ffffff;
    border: 1px solid #ddd;
    gridline-color: #ccc;
    font-size: 13px;
    selection-background-color: #4e8cff;
    selection-color: #ffffff;
}

QHeaderView::section {
    background: #4e8cff;
    color: #ffffff;
    padding: 5px;
    border: 1px solid #ddd;
    font-weight: bold;
}

QTableView QTableCornerButton::section {
    background: #4e8cff;
    border: 1px solid #ddd;
}

/* Style des radiobuttons et checkboxes */
QRadioButton, QCheckBox {
    font-size: 13px;
    padding: 5px;
}

QRadioButton::indicator, QCheckBox::indicator {
    width: 16px;
    height: 16px;
}

QRadioButton::indicator:checked, QCheckBox::indicator:checked {
    background: #4e8cff;
    border: 1px solid #375e97;
}

/* Style des labels */
QLabel {
    font-size: 14px;
    font-weight: bold;
    color: #333;
}

/* Style des sections spécifiques */
#searchBar {
    background: #ffffff;
    border: 2px solid #ddd;
    border-radius: 20px;
    padding: 5px 15px;
    font-size: 14px;
}

QPushButton#searchButton {
    background: #4e8cff;
    color: #ffffff;
    border: none;
    border-radius: 50%;
    width: 40px;
    height: 40px;
    font-size: 16px;
    font-weight: bold;
}

QPushButton#searchButton:hover {
    background: #6aa6ff;
}
