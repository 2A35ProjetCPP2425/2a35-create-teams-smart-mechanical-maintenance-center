    background-color: #0066cc; /* Blue header */
    color: #ffffff; /* White text */
    padding: 8px;
    font-weight: bold;
    border: 1px solid #d0d5db;
}

/* QListWidget Style */
QListWidget {
    background-color: #ffffff; /* White background */
    border: 1px solid #d0d5db;
    color: #333; /* Dark gray text */
    padding: 5px;
}

/* QLabel Style */
QLabel {
    color: #333; /* Dark gray text */
    font-weight: bold;
}

/* QComboBox Style */
QComboBox {
    background-color: #ffffff; /* White background */
    border: 1px solid #d0d5db;
    padding: 5px;
    border-radius: 5px;
    color: #333;
}

QComboBox::drop-down {
    background-color: #0066cc; /* Blue background for dropdown */
}

