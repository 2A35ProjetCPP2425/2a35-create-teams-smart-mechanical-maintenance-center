                <string notr="true">/* Styles généraux pour la fenêtre principale */
QMainWindow {
    background-color: #ffffff; /* Background blanc */
    border: none;
    padding: 10px;
}

/* Champs de texte */
QLineEdit {
    background-color: #ffffff; /* Blanc */
    border: 1px solid #cccccc; /* Contour gris clair */
    border-radius: 5px;
    padding: 5px;
    font-size: 14px;
    color: #333333;
}

QLineEdit:focus {
    border: 1px solid #4a90e2; /* Contour bleu clair au focus */
    outline: none;
}

/* Boutons */
QPushButton {
    background-color: #ffffff; /* Blanc */
    border: 1px solid #cccccc; /* Contour gris clair */
    color: #333333; /* Texte gris foncé */
    padding: 8px 16px;
    font-size: 14px;
    border-radius: 5px;
    transition: all 0.3s ease;
}

QPushButton:hover {
    background-color: #f0f0f0; /* Gris très clair au survol */
    border: 1px solid #4a90e2; /* Contour bleu clair au survol */
    color: #000000; /* Texte noir */
}

QPushButton:pressed {
    background-color: #e6e6e6; /* Gris plus foncé quand pressé */
}

/* Tables */
QTableWidget, QTableView {
    background-color: #ffffff; /* Fond blanc */
    border: 1px solid #cccccc; /* Contour gris clair */
    gridline-color: #cccccc; /* Lignes de la grille */
    border-radius: 5px;
    font-size: 14px;
}

QHeaderView::section {
    background-color: #f0f0f0; /* Gris très clair */
    color: #333333; /* Texte gris foncé */
    padding: 5px;
    border: 1px solid #cccccc; /* Contour gris clair */
    text-align: center;
}

QTableWidget QTableCornerButton::section {
    background-color: #f0f0f0;
}

/* Items des tableaux */
QTableView::item {
    border: 1px solid #cccccc; /* Contour gris clair */
    padding: 5px;
}

QTableView::item:selected {
    background-color: #4CAF50; /* Vert clair pour les sélections */
    color: #ffffff; /* Texte blanc */
}

/* Labels */
QLabel {
    font-size: 14px;
    color: #333333; /* Texte gris foncé */
    padding: 2px;
}

/* GroupBoxes */
QGroupBox {
    border: 1px solid #cccccc; /* Contour gris clair */
    border-radius: 5px;
    margin-top: 20px;
    padding: 10px;
    background-color: #ffffff; /* Fond blanc */
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    padding: 0 3px;
    color: #333333; /* Texte gris foncé */
    background-color: #ffffff; /* Fond blanc */
    border-radius: 3px;
    font-weight: bold;
}

/* Radio buttons */
QRadioButton {
    font-size: 14px;
    color: #333333; /* Texte gris foncé */
    padding: 2px;
}

QRadioButton::indicator {
    border: 1px solid #cccccc; /* Contour gris clair */
    border-radius: 5px;
    width: 16px;
    height: 16px;
    background-color: white; /* Fond blanc */
}

QRadioButton::indicator:checked {
    background-color: #4CAF50; /* Fond vert clair */
}

/* Combobox */
QComboBox {
    background-color: #ffffff; /* Fond blanc */
    border: 1px solid #cccccc; /* Contour gris clair */
    border-radius: 5px;
    padding: 5px;
    font-size: 14px;
}

QComboBox::drop-down {
    border: none;
    width: 25px;
    background: #f0f0f0; /* Fond gris très clair */
}

QComboBox QAbstractItemView {
    background: white;
    border: 1px solid #cccccc;
}

/* Bouton PDF spécifique */
QPushButton#exportPDF {
    background-color: #ffffff; /* Fond blanc */
    border: 1px solid #cccccc; /* Contour gris clair */
    color: #007BFF; /* Bleu pour le texte */
    font-size: 14px;
    padding: 8px 16px;
    border-radius: 5px;
}

QPushButton#exportPDF:hover {
    background-color: #f0f0f0; /* Gris clair au survol */
    border-color: #007BFF; /* Contour bleu clair */
    color: #0056b3; /* Bleu plus foncé */
}
